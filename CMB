    <script>
        function showMainMenu() {
            var wid = window.innerWidth;
            if (wid <= 769) {
                var mnuEle = document.getElementById("menu").className;
                if (mnuEle === "collapse") {
                    document.getElementById("menu").className = "mobMenu";
                }
                else {
                    document.getElementById("menu").className = "collapse";
                }
            }
        }
       // window.onload = getBrowserWidth();
        function getBrowserWidth() {
            var wid = window.innerWidth;
            document.getElementById("bWidth").innerHTML = wid + " px &nbsp;&nbsp; Important web links.";
        }
        function goHome() {
            window.location.href = "/";
        }
        function showLogin() {
            var x = document.getElementById("loginbox");
            if (x.style.display === 'block') {
                x.style.display = 'none';
            } else {
                x.style.display = 'block';
            }
        }
        function hideLogin() {
            var x = document.getElementById("loginbox");
            if (x.style.display === 'block') {
                x.style.display = 'none';
            }
        }
        function readEsc(e) {
            if (e.keyCode === 27)
                hideLogin();
            if (e.keyCode === 13) {
                //alert("It will logout you");
            }
            //
            // window.applicationCache.abort();
        }
        function clientLogin(id){
            var session = document.getElementById("userName1").innerHTML;
            if (session.trim() === 'Not logged in.') {
                var x = document.getElementById("loginbox");
                var box = document.getElementById("HfModelId");
                box.value = id;
                if (x.style.display === 'block') {
                    x.style.display = 'none';
                } else {
                    x.style.display = 'block';
                }
                document.body.scrollTop = 0; // For Chrome, Safari and Opera 
                document.documentElement.scrollTop = 0; // For IE and Firefox
            } else {
                var url = window.location.href;
                var a;
                //if (url.indexOf("clientArea") !== "-1") {
                //    a = "AccessoriesDescription.aspx?ModelID=" + id;
                //}
                //else {
                //    a = "clientArea/AccessoriesDescription.aspx?ModelID=" + id;
                //}
                a = "../clientArea/AccessoriesDescription.aspx?ModelID=" + id;
                window.location.href = a;
            }
        }

